@using RS_SEMINARSKI.ViewModels
@model MuzikaPrikaziZanroveBendovaVM

<style>

    thead td {
        background-color: #d9f2d9;
        text-align: center;
        font-weight: bold;
    }


</style>



<table class="table">

    <thead>
        @*<td>Bend ID</td>*@
        <td>Naziv zanra</td>
    </thead>
    @foreach (var x in Model.BendZanrovi)
    {
        <tr>
            @*<td>@x.BendID</td>*@
            <td>@x.NazivZanra</td>
        </tr>
    }



</table>
